{
    "name": "iis-start",
    "private": true,
    "engines": {
        "node": ">=14.*",
        "npm": ">=7.*"
    },
    "scripts": {
        "start": "npm run watch",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "eslint": "eslint assets --ext .jsx --ext .js",
        "stylelint": "./node_modules/.bin/stylelint \"assets/**/*.scss\"",
        "lint": "npm run eslint && npm run stylelint"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.22.15",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.23.2",
        "@internetstiftelsen/eslint-config-react": "^3.0.0",
        "@internetstiftelsen/stylelint-config": "^3.0.0",
        "dotenv-webpack": "^8.0.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-webpack-plugin": "^4.0.1",
        "laravel-mix": "^6.0.49",
        "postcss": "^8.4.31",
        "postcss-loader": "^7.3.3",
        "postcss-scss": "^4.0.9",
        "prettier": "^3.0.3",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "stylelint-webpack-plugin": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-bugsnag-plugins": "^1.8.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@bugsnag/js": "^7.21.0",
        "@internetstiftelsen/styleguide": "^2.26.19"
    }
}
